 @model int
@{ 
    ViewBag.Title = "404 Error";
}
    <div class="alert alert-danger mt-1 mb-1 " >
        <h4>404 Not Found Error</h4>
        <hr />
        <h5>Employee with Id @Model  can not be found</h5>
    </div>
    <a asp-controller="Home" asp-action="Employees" class="btn btn-outline-success" style="width:auto">
        Click here to view the list of all employees 
    </a>